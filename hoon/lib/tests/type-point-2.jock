compose
  class Point(x:@ y:@) {
    // return an instance
    add(p:(x:@ y:@)) -> Point {
      (x + p.x
       y + p.y)
    }
    // return a cell
    add2(p:(x:@ y:@)) -> (@ @) {
      (x + p.x
       y + p.y)
    }
    // return an atom
    inc(q:@) -> @ {
      +(q)
    }
  }
;

let origin = Point(12 23);
origin.add(30 19)

