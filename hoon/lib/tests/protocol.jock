protocol Arithmetic {
  func add(#) -> #;
  func sub(#) -> #;
};

object Point: Arithmetic {
  p:(x:@ y:@);
  init(q:(x:@ y:@)) -> Point {
    p.x = q.x;
    p.y = q.y;
  }
  add(q:Point) -> Point {
    Point(((p.x + q.x) (p.y + q.y)))
  };
  sub(q:Point) -> Point {
    Point(((p.x - q.x) (p.y - q.y)))
  };
};

