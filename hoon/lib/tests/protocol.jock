protocol Arithmetic {
  add(#) -> #;
  sub(#) -> #;
};

type Point(x:@ y:@) implements Arithmetic {
  init(q:(x:@ y:@)) -> Point {
    x = q.x;
    y = q.y;
  };
  add(q:Point) -> Point {
    new Point(((x + q.x) (y + q.y)));
  };
  sub(q:Point) -> Point {
    new Point(((x - q.x) (y - q.y)));
  };
};

let origin:Point = new Point(0 0);
let vector:Point = new Point(1 2);

origin.add(vector)
