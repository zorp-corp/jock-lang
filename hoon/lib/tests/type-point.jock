compose
  class Point(x:@ y:@) {
    // load() is the required constructor
    load(p:Point) -> Point {
      // the return must match the output shape
      (p.x p.y)
    }
    add(p:Point) -> Point {
      // we don't have infix operators yet so this is just a weird hack
      (x p.y)
    }
  }
; // end compose

// use the implicit load() constructor
let origin = Point(50 60);

origin
